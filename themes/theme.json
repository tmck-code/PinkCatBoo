{
	"name": "Pink Cat Boo",
	"auth": "Fiona Fan",
	"type": "dark",
	"colors": {
		"focusBorder": "--geraldine: #fe7c8e;",
		"activityBar.background": "--ebony-clay: #202330;",
		"activityBar.foreground": "--pig-pink: #fde3e8;",
		"activityBarBadge.background": "--geraldine: #fe7c8e;",
		"button.background": "--geraldine: #fe7c8e;",
		"dropdown.background": "--ebony-clay: #202330;",
		"editor.background": "--ebony-clay: #202330;",
		"editor.foreground": "--lavender-blush: #FFF0F5;",
		"editor.selectionBackground": "--livid-brown: #472541;",
		"editor.selectionHighlightBackground": "--abbey: #575b61;",
		"editor.lineHighlightBackground": "--livid-brown: #472541;",
		"editor.wordHighlightBackground": "--victoria: #4747a1;",
		"editor.wordHighlightStrongBackground": "--blue-marguerite: #6767ce;",
		"editorBracketMatch.border": "--raw-sienna: #d0963a;",
		"editorCursor.foreground": "--lavender-blush: #ffedf0;",
		"editorError.foreground": "--hot-pink: #ff62a5;",
		"editorWarning.foreground": "--shamrock: #3bc089;",
		"editorGroup.background": "--cod-gray: #1e1e1e;",
		"editorGroupHeader.tabsBackground": "--ebony-clay: #2d2f42;",
		"extensionButton.prominentForeground": "--white: #ffffff;",
		"extensionButton.prominentBackground": "--geraldine: #fe7c8e;",
		"extensionButton.prominentHoverBackground": "--geraldine: #fe7c8e;",
		"input.background": "--ebony-clay: #202330;",
		"inputOption.activeBorder": "--geraldine: #fe7c8e;",
		"list.activeSelectionBackground": "--raw-sienna: #d0963a;",
		"list.focusBackground": "--dove-gray: #707070;",
		"list.inactiveSelectionBackground": "--yellow-metal: #765d36;",
		"list.hoverBackground": "--comet: #565970;",
		"list.highlightForeground": "--white: #FFFFFF;",
		"list.errorForeground": "--hot-pink: #ff62a5;",
		"menu.separatorBackground": "--mandys-pink: #f3b4bf;",
		"notificationCenter.border": "--mandys-pink: #f3b4bf;",
		"notificationCenterHeader.background": "--mandys-pink: #f3b4bf;",
		"notificationCenterHeader.foreground": "--white: #ffffff;",
		"notificationLink.foreground": "--geraldine: #fe7c8e;",
		"notifications.background" : "--salt-box: #6d5d72;",
		"notifications.foreground": "--mandys-pink: #f3b4bf;",
		"peekView.border": "--raw-sienna: #d0963a;",
		"panelTitle.activeForeground": "--white: #ffffff;",
		"scrollbarSlider.background": "--mandys-pink: #f3b4bf;",
		"scrollbarSlider.hoverBackground": "--mandys-pink: #f3b4bf;",
		"scrollbarSlider.activeBackground": "--kobi: #e9a6b2;",
		"statusBar.background": "--mandys-pink: #f3b4bf;",
		"statusBar.debuggingBackground": "--mandys-pink: #f3b4bf;",
		"statusBar.noFolderBackground": "--mandys-pink: #f3b4bf;",
		"sideBar.background": "--ebony-clay: #2d2f42;",
		"sideBarSectionHeader.background": "--ebony-clay: #202330;",
		"tab.inactiveBackground": "--ebony-clay: #2d2f42;",
		"tab.border": "--manatee: #9498a1;",
		"tab.inactiveForeground": "--manatee: #9498a1;",
		"tab.activeForeground": "--geraldine: #fe7c8e;",
		"tab.activeBorderTop": "--geraldine: #fe7c8e;",
		"terminal.ansiWhite": "--white: #ffffff;",
		"titleBar.activeBackground": "--ebony-clay: #2d2f42;",
	},
	"tokenColors": [
		{
			"settings": {
				"background": "--ebony-clay: #202330;",
				"foreground": "--lavender-blush: #FFF0F5;",
			}
		},
		{
			"name": "By uonick",
			"settings": {
				"background": "--shark: #202025;",
				"foreground": "--pumice: #c5c8c6;","
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"background": "--cod-gray: #1e1e1e;",
				"foreground": "--pumice: #C5C8C6;",
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": { "foreground": "--xanadu: #6D7A72;" }
		},
		{
			"name": "String",
			"scope": "string",
			"settings": { "foreground": "--astra: #FAE8B6;" }
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": { "foreground": "--wild-strawberry: #FF38A2;" }
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": { "foreground": "--silver-tree: #58B896;" }
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other",
			"settings": { "foreground": "--tickle-me-pink: #FF7F9D;" }
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": { "foreground": "--wild-strawberry: #FF4791;" }
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": { "foreground": "--sunglow: #FEC831;" }
		},
		{
			"name": "Storage Type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "--moon-raker: #DCBFF2;",
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.class",
			"settings": {
				"fontStyle": "underline",
				"foreground": "--illusion: #F5A6C6;",
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "--shamrock: #37e884;","
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": { "foreground": "--golden-tainoi: #ffc85b;" }
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "--celery: #9CD162;",
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": { "foreground": "--violet-red: #FA508C;" }
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "bold",
				"foreground": "--cornflower: #A2C2EB;",
			}
		},
		{
			"name": "Class Variable",
			"scope": "variable.other, variable.js, punctuation.separator.variable",
			"settings": { "foreground": "--sea-pink: #EBA4AC;" }
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": { "foreground": "--white: #FFFFFF;" }
		},
		{
			"name": "Normal Variable",
			"scope": "variable.other.php, variable.other.normal",
			"settings": { "foreground": "--sea-pink: #EBA4AC;" }
		},
		{
			"name": "Function Call Variable",
			"scope": "variable.other.property",
			"settings": { "foreground": "--moon-raker: #DCBFF2;" }
		},
		{
			"name": "Tag",
			"scope": "meta.tag",
			"settings": { "foreground": "--lavender-blush: #FFF0F5;" }
		},
		{
			"name": "Tag Name",
			"scope": "entity.name.tag",
			"settings": { "foreground": "--violet-red: #FA508C;" }
		},
		{
			"name": "Doctype",
			"scope": "meta.doctype, meta.tag.sgml-declaration.doctype, meta.tag.sgml.doctype",
			"settings": { "foreground": "--viking: #73b5d7;" }
		},
		{
			"name": "Tag Inline Source",
			"scope": "meta.tag.inline source, text.html.php.source",
			"settings": { "foreground": "--viking: #73b5d7;" }
		},
		{
			"name": "Tag Attribute",
			"scope": "entity.other.attribute-name, meta.tag punctuation.definition.string",
			"settings": { "foreground": "--mauve: #E6A1FF;" }
		},
		{
			"name": "Tag Value",
			"scope": "meta.tag string -source -punctuation, text source text meta.tag string -punctuation",
			"settings": { "foreground": "--hippie-blue: #6089B4;" }
		},
		{
			"name": "Meta Brace",
			"scope": "punctuation.section.embedded -(source string source punctuation.section.embedded), meta.brace.erb.html",
			"settings": { "foreground": "--old-gold: #D0B344;" }
		},
		{
			"name": "HTML ID",
			"scope": "meta.toc-list.id",
			"settings": { "foreground": "--viking: #73b5d7;" }
		},
		{
			"name": "HTML Tags",
			"scope": "punctuation.definition.tag.html, punctuation.definition.tag.begin, punctuation.definition.tag.end",
			"settings": { "foreground": "--lavender-blush: #FFF0F5;" }
		},
		{
			"name": "CSS Library class type",
			"scope": "support.type, support.class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "--cornflower: #94AFE8;",
			}
		},
		{
			"name": "Template Definition",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded.coffee"
			],
			"settings": { "foreground": "--raw-sienna: #D08442;" }
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": { "foreground": "--pumice: #C5C8C6;" }
		},
		{
			"name": "PHP Single Quote HMTL Fix",
			"scope": "punctuation.definition.string.end.php, punctuation.definition.string.begin.php",
			"settings": { "foreground": "--catskill-white: #e1ecf2;" }
		},
		{
			"name": "PHP Parenthesis HMTL Fix",
			"scope": "source.php.embedded.line.html",
			"settings": { "foreground": "--boulder: #787878;" }
		},
		{
			"name": "PHP Punctuation Embedded",
			"scope": "punctuation.section.embedded.begin.php, punctuation.section.embedded.end.php",
			"settings": { "foreground": "--porsche: #ec9a5e;" }
		},
		{
			"name": "SQL",
			"scope": "keyword.other.DML.sql",
			"settings": { "foreground": "--old-gold: #D0B344;" }
		},
		{
			"name": "diff: header",
			"scope": "meta.diff, meta.diff.header",
			"settings": {
				"background": "--pirate-gold: #b58900;",
				"fontStyle": "italic",
				"foreground": "--willow-brook: #E0EDDD;",
			}
		},
		{
			"name": "diff: deleted",
			"scope": "markup.deleted",
			"settings": {
				"background": "--parchment: #f0e8cf;",
				"foreground": "--punch: #dc322f;",
			}
		},
		{
			"name": "diff: changed",
			"scope": "markup.changed",
			"settings": {
				"background": "--parchment: #f0e8cf;",
				"foreground": "--cerise-red: #e53c70;",
			}
		},
		{
			"name": "diff: inserted",
			"scope": "markup.inserted",
			"settings": {
				"background": "--parchment: #f0e8cf;",
				"foreground": "--elm: #219186;",
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": { "foreground": "--wisteria: #9e6dbc;" }
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": { "foreground": "--viking: #73b5d7;" }
		},
		{
			"name": "Markup Styling",
			"scope": "markup.bold, markup.italic",
			"settings": { "foreground": "--hippie-blue: #6089B4;" }
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": { "foreground": "--rose: #FF0080;" }
		},
		{
			"name": "Markup Headings",
			"scope": "markup.heading",
			"settings": { "foreground": "--old-gold: #D0B344;" }
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading.setext",
			"settings": { "foreground": "--old-gold: #D0B344;" }
		},
		{
			"scope": "token.info-token",
			"settings": { "foreground": "--cornflower-blue: #6796e6;" }
		},
		{
			"scope": "token.warn-token",
			"settings": { "foreground": "--tulip-tree: #e5ac40;" }
		},
		{
			"scope": "token.error-token",
			"settings": { "foreground": "--sunset-orange: #f44747;" }
		},
		{
			"scope": "token.debug-token",
			"settings": { "foreground": "--medium-purple: #b267e6;" }
		}
	]
}
